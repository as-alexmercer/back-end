{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Trabajo Pr√°ctico Profesional, FIUBA, 2020",
  "engines": {
    "node": "12.14.0"
  },
  "main": "index.ts",
  "dependencies": {
    "@types/bluebird": "3.5.29",
    "@types/express": "4.17.2",
    "@types/glob": "7.1.1",
    "@types/graphql": "14.5.0",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/node": "13.1.4",
    "@types/node-fetch": "2.5.4",
    "@types/sequelize": "4.28.8",
    "@types/supertest": "2.0.8",
    "@types/validator": "12.0.1",
    "apollo-server": "2.9.15",
    "apollo-server-express": "2.9.15",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "glob": "7.1.6",
    "graphql": "14.5.8",
    "graphql-depth-limit": "1.1.0",
    "graphql-import": "0.7.1",
    "graphql-import-node": "0.0.4",
    "http-status-codes": "1.4.0",
    "husky": "3.1.0",
    "lodash": "4.17.15",
    "node": "12.14.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "pg": "7.17.0",
    "pg-hstore": "2.3.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "sequelize": "5.21.3",
    "sequelize-cli-typescript": "3.2.0-c",
    "sequelize-typescript": "1",
    "typescript-logging": "0.6.4"
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "babel-cli": "6.26.0",
    "babel-jest": "24.9.0",
    "@babel/core": "7.0.0",
    "eslint": "6.8.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.5.0",
    "coveralls": "3.0.9",
    "jest": "24.9.0",
    "superagent": "5.1.3",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.18.0",
    "tslint-config-fiuba-laboral-v2": "1.0.3",
    "typescript": "3.7.4"
  },
  "scripts": {
    "dev": "yarn build && concurrently --kill-others \"tsc -w -p .\" \"nodemon dist/src/index.js -e js\" || true",
    "debug": "yarn build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e js\" || true",
    "build": "yarn clean && tsc -p .",
    "start": "node dist/src/index.js",
    "test": "jest --coverage --runInBand",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'test/**/*.ts'",
    "lint": "yarn lint:src && yarn lint:test",
    "stash": "git stash -u --keep-index",
    "unstash": "git stash pop",
    "check-unstaged": "bash scripts/check_unstaged.sh",
    "clean": "rimraf dist",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "db:migrate": "yarn build && node_modules/.bin/sequelize db:migrate",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "db:test:migrate": "yarn build && NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "db:test:create": "NODE_ENV=test node_modules/.bin/sequelize db:create",
    "db:test:drop": "NODE_ENV=test node_modules/.bin/sequelize db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiuba-laboral-v2/back-end.git"
  },
  "author": "Dylan Alvarez, Manuel Llauro, Sebastian Blanco",
  "license": "ISC",
  "bugs": {
    "url": "https://fiuba-laboral-v2.atlassian.net"
  },
  "homepage": "https://github.com/fiuba-laboral-v2/back-end#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-unstaged && yarn lint && yarn test"
    }
  }
}
