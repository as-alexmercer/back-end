{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Trabajo Pr√°ctico Profesional, FIUBA, 2020",
  "engines": {
    "node": "12.14.0"
  },
  "main": "index.ts",
  "dependencies": {
    "@types/express": "4.17.2",
    "@types/supertest": "2.0.8",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "glob": "7.1.6",
    "http-status-codes": "1.4.0",
    "husky": "3.1.0",
    "lodash": "4.17.15",
    "node": "12.14.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "@types/sequelize": "4.28.8",
    "sequelize": "4.41.0",
    "sequelize-cli": "5.5.1",
    "pg": "7.4.3",
    "typescript-logging": "0.6.4"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@types/jest": "24.0.0",
    "babel-cli": "6.26.0",
    "babel-jest": "24.9.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.5.0",
    "coveralls": "3.0.9",
    "jest": "24.9.0",
    "superagent": "5.1.3",
    "supertest": "4.0.2",
    "ts-node": "8.5.4",
    "tslint": "5.18.0",
    "typescript": "3.7.4",
    "tslint-config-fiuba-laboral-v2": "1.0.1"
  },
  "scripts": {
    "dev": "yarn build && concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/src/index.js -e js\" || true",
    "debug": "yarn build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e js\" || true",
    "build": "tsc -p src",
    "start": "node dist/src/index.js",
    "test": "jest --coverage",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'test/**/*.ts'",
    "lint": "yarn lint:src && yarn lint:test",
    "clean": "rimraf dist",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiuba-laboral-v2/back-end.git"
  },
  "author": "Dylan Alvarez, Manuel Llauro, Sebastian Blanco",
  "license": "ISC",
  "bugs": {
    "url": "https://fiuba-laboral-v2.atlassian.net"
  },
  "homepage": "https://github.com/fiuba-laboral-v2/back-end#readme",
  "husky": {
    "hooks": {
      "pre-commit": "git stash -u --keep-index && yarn lint && git stash pop"
    }
  }
}
