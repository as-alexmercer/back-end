{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Trabajo Pr√°ctico Profesional, FIUBA, 2020",
  "engines": {
    "node": "12.14.0",
    "npm": "6.11.3"
  },
  "main": "index.ts",
  "dependencies": {
    "@types/express": "4.17.2",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "glob": "7.1.6",
    "http-status-codes": "1.4.0",
    "lodash": "4.17.15",
    "node": "12.14.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "typescript-logging": "0.6.4"
  },
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "5.2.7",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "3.5.0",
    "mocha": "6.2.2",
    "ts-node": "^8.5.4",
    "tslint": "5.18.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "dev": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/index.js -e ts\" || true",
    "debug": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "npm run lint && npm run lint:test && mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc -p src",
    "start": "node dist/index.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiuba-laboral-v2/back-end.git"
  },
  "author": "Dylan Alvarez, Manuel Llauro, Sebastian Blanco",
  "license": "ISC",
  "bugs": {
    "url": "https://fiuba-laboral-v2.atlassian.net"
  },
  "homepage": "https://github.com/fiuba-laboral-v2/back-end#readme"
}
